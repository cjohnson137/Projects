

// Create Gallery Section

var galleryWin = Ti.UI.createWindow({
	
	backgroundColor: "#3A00FF",
	layout: "horizontal"
		
});



var testImage = Ti.UI.createImageView({
	
	image: "pics/2010-12-12 15.08.11.jpg",
	left: 10,
	right: 10,
	top: 20,
	
});
	
	galleryWin.add(testImage);

	galleryWin.open();
	
